<div class="jumbotron jumbotron-fluid" id="front-page">
    <div class="container" id="center-div">
        <form action="/users/{{userId}}/lists/{{list._id}}?_method=PUT" method="POST">
            <div class="form-group">
                <label for="name">Name</label>
                <input type="text" class="form-control" name="name" value="{{list.name}}">
            </div>
            <div class="form-group">
                <label for="qty">Qty</label>
                <input type="number" class="form-control" name="qty" value="{{list.price}}" step="any">
            </div>
             <div class="form-group">
                <label for="note">Note</label>
                <input type="text" class="form-control" name="note" value="{{list.note}}" step="any">
            </div>
            <div class="form-group" id="edit">
                <button type="submit" class="btn btn-primary">Edit</button>
                     <a href="/users/{{userId}}/lists">
             <button type="button" class="btn btn-primary">
            <span class="glyphicon glyphicon glyphicon-backward"></span>
           Back</button></a>
            </div>
         
        </form>
        </div>
        </div>