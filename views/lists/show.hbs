<div class="jumbotron jumbotron-fluid" id="front-page">
    <div class="container">
        <h2>View List </h2>
        <table class ="table">
        <thead class="thead-inverse">
            <tr>
                <th>Name</th>
                <th>Quantity</th>
                <th>Note</th>
                </tr>
        </thead>
        <tbody>
            <tbody>
                <tr>
               <th>{{list.name}}</th>
               <th>{{list.qty}}</th>
