            </tbody>
        </tbody>
        </table>
        <div>
            <a href="/users/{{userId}}/lists/{{list._id}}/edit">
             <button type="button" class="btn btn-primary">
                  <span class="glyphicon glyphicon glyphicon-scissors">
              </span>
            Edit</button></a>
                <a href="/users/{{userId}}/lists/{{list._id}}/delete">
             <button type="button" class="btn btn-primary">
            <span class="glyphicon glyphicon glyphicon-trash">
              </span>
            Delete</button></a>
                <a href="/users/{{userId}}/lists">
             <button type="button" class="btn btn-primary">
                  <span class="glyphicon glyphicon glyphicon-backward">
              </span>
           Back</button></a>
            </div>
    </div>
</div>