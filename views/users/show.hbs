<div class="jumbotron jumbotron-fluid" id="front-page">
  <div class="container">
    <div class="row">
     <div class="col-md-3" >
                <img src="../images/icon.jpg" class="img-responsive" id="logo" alt="logo">
            </div>
            <div class="col-md-6" id="center-div">
                <h3>{{user.name}}'s Profile</h3>
            </div>
          </div>
  <table class="table">
    <tbody>
      <tr>
        <td>Name: {{user.name}}</td>
        </tr>
        <tr>
        <td>Phone: {{user.phone}}</td>
        </tr>
        <tr>
        <td>Email: {{user.email}}</td>
         </tr>
        <tr>
        <td>Street: {{user.street}}</td>
         </tr>
        <tr>
        <td>City: {{user.city}}</td>
         </tr>
        <tr>
        <td>State: {{user.state}}</td>
         </tr>
        <tr>
        <td>Zipcode: {{user.zipcode}}</td>
         </tr>
        <tr>
        {{!-- <td>Store: {{user.store}}</td> --}}
          </tr>        
     </tbody>
   </table>
    <div>
      <a href="/users"><button type="button" class="btn btn-primary">
        <span class="glyphicon glyphicon glyphicon-backward">
              </span> Back to Users</button></a>
      <a href="/users/{{user._id}}/edit">
            <button type="button" class="btn btn-primary">
               <span class="glyphicon glyphicon glyphicon-scissors">
              </span>
              Edit</button></a>
      <a href="/users/{{user._id}}/delete"> 
            <button type="button" class="btn btn-primary">
               <span class="glyphicon glyphicon glyphicon-trash">
              </span>
              Delete</button></a>
    </div>
  </div>
</div>