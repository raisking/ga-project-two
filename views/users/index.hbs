<div>
    <h1>Shopping List</h1>

    <h3>Users:</h3>
    <div>
        <a href="/users/new">Add New User</a>
    </div>
    {{#each users}}
        <div>
            {{this.name}} - {{this.phone}}
            <a href="/users/{{this._id}}">{{this.name}} Info</a>
            <a href="/users/{{this._id}}/lists">{{this.name}}'s Lists</a>
        </div>
    {{/each}}
</div>